[
  {
    "commands": [
      {
        "cloneInstruction": {},
        "name": "clone"
      },
      {
        "cacheInstruction": {
          "fingerprintScripts": [
            "cat Cargo.lock"
          ],
          "folder": "$CARGO_HOME/registry"
        },
        "name": "cargo"
      },
      {
        "name": "build",
        "scriptInstruction": {
          "scripts": [
            "cargo build"
          ]
        }
      },
      {
        "name": "test",
        "scriptInstruction": {
          "scripts": [
            "cargo test"
          ]
        }
      },
      {
        "name": "before_cache",
        "scriptInstruction": {
          "scripts": [
            "rm -rf $CARGO_HOME/registry/index"
          ]
        }
      },
      {
        "name": "Upload 'cargo' cache",
        "uploadCacheInstruction": {
          "cacheName": "cargo"
        }
      }
    ],
    "environment": {
      "CIRRUS_OS": "linux"
    },
    "instance": {
      "@type": "type.googleapis.com/org.cirruslabs.ci.services.cirruscigrpc.ContainerInstance",
      "cpu": 2,
      "image": "rust:latest",
      "memory": 4096
    },
    "metadata": {
      "properties": {
        "allow_failures": "false",
        "experimental": "false",
        "indexWithinBuild": "0",
        "skip_notifications": "false",
        "timeout_in": "3600",
        "trigger_type": "AUTOMATIC"
      },
      "uniqueLabels": [
        "container:rust:latest"
      ]
    },
    "name": "test"
  },
