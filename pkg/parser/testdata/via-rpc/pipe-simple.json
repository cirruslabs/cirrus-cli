[
  {
    "commands": [
      {
        "cloneInstruction": {},
        "name": "clone",
        "properties": {
          "image": "squidfunk/mkdocs-material:latest"
        }
      },
      {
        "name": "install",
        "scriptInstruction": {
          "scripts": [
            "pip install --upgrade pymdown-extensions"
          ]
        }
      },
      {
        "name": "build",
        "scriptInstruction": {
          "scripts": [
            "mkdocs build"
          ]
        }
      },
      {
        "name": "validate",
        "properties": {
          "image": "liche:latest"
        },
        "scriptInstruction": {
          "scripts": [
            "liche --document-root=site --recursive site/"
          ]
        }
      }
    ],
    "environment": {
      "CIRRUS_OS": "linux"
    },
    "instance": {
      "@type": "type.googleapis.com/org.cirruslabs.ci.services.cirruscigrpc.PipeInstance",
      "cpu": 4,
      "memory": 12288
    },
    "metadata": {
      "properties": {
        "allow_failures": "false",
        "auto_cancellation": "false",
        "experimental": "false",
        "indexWithinBuild": "0",
        "skip_notifications": "false",
        "timeout_in": "3600",
        "trigger_type": "AUTOMATIC"
      }
    },
    "name": "Build and Validate Links"
  }
]
