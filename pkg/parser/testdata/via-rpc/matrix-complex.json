[
  {
    "commands": [
      {
        "cloneInstruction": {},
        "name": "clone"
      },
      {
        "cacheInstruction": {
          "folder": "$HOME/.gradle/caches",
          "reuploadOnChanges": true
        },
        "name": "gradle"
      },
      {
        "name": "check",
        "scriptInstruction": {
          "scripts": [
            "gradle build idea -s -PcheckJava6Compatibility"
          ]
        }
      },
      {
        "name": "release",
        "scriptInstruction": {
          "scripts": [
            "gradle ciPerformRelease"
          ]
        }
      },
      {
        "name": "cleanup_before_cache",
        "scriptInstruction": {
          "scripts": [
            "rm -rf ~/.gradle/caches/$GRADLE_VERSION/",
            "find ~/.gradle/caches/ -name \"*.lock\" -type f -delete"
          ]
        }
      },
      {
        "name": "Upload 'gradle' cache",
        "uploadCacheInstruction": {
          "cacheName": "gradle"
        }
      }
    ],
    "environment": {
      "CIRRUS_OS": "linux"
    },
    "instance": {
      "@type": "type.googleapis.com/org.cirruslabs.ci.services.cirruscigrpc.ContainerInstance",
      "cpu": 2,
      "image": "gradle:4.4.1-jdk8",
      "memory": 4096
    },
    "metadata": {
      "properties": {
        "allow_failures": "false",
        "auto_cancellation": "false",
        "experimental": "false",
        "indexWithinBuild": "0",
        "skip_notifications": "false",
        "timeout_in": "3600",
        "trigger_type": "AUTOMATIC"
      },
      "uniqueLabels": [
        "container:gradle:4.4.1-jdk8"
      ]
    },
    "name": "check"
