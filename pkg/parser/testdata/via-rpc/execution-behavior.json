[
  {
    "commands": [
      {
        "cloneInstruction": {},
        "name": "clone"
      },
      {
        "name": "exit",
        "scriptInstruction": {
          "scripts": [
            "exit 1"
          ]
        }
      },
      {
        "cacheInstruction": {
          "folder": "node_modules",
          "reuploadOnChanges": true
        },
        "executionBehaviour": "ALWAYS",
        "name": "node"
      },
      {
        "executionBehaviour": "ON_FAILURE",
        "name": "debug1",
        "scriptInstruction": {
          "scripts": [
            "echo \"Handle failure!\""
          ]
        }
      },
      {
        "executionBehaviour": "ON_FAILURE",
        "name": "debug2",
        "scriptInstruction": {
          "scripts": [
            "echo \"Handle failure once again!\""
          ]
        }
      },
      {
        "executionBehaviour": "ALWAYS",
        "name": "echo1",
        "scriptInstruction": {
          "scripts": [
            "echo \"Foo!\""
          ]
        }
      },
      {
        "executionBehaviour": "ALWAYS",
        "name": "echo2",
        "scriptInstruction": {
          "scripts": [
            "echo \"Bar!\""
          ]
        }
      },
      {
        "executionBehaviour": "ALWAYS",
        "name": "Upload 'node' cache",
        "uploadCacheInstruction": {
          "cacheName": "node"
        }
      }
    ],
    "environment": {
      "CIRRUS_OS": "linux"
    },
    "instance": {
      "@type": "type.googleapis.com/org.cirruslabs.ci.services.cirruscigrpc.ContainerInstance",
      "cpu": 2,
      "image": "alpine:latest",
      "memory": 4096
    },
    "metadata": {
      "properties": {
        "allow_failures": "false",
        "experimental": "false",
        "indexWithinBuild": "0",
        "timeout_in": "3600",
        "trigger_type": "AUTOMATIC"
      }
    },
    "name": "main"
  }
]
