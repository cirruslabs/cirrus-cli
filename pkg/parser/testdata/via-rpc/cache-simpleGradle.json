[
  {
    "commands": [
      {
        "cloneInstruction": {},
        "name": "clone"
      },
      {
        "name": "main",
        "scriptInstruction": {
          "scripts": [
            "./gradlew check"
          ]
        }
      },
      {
        "cacheInstruction": {
          "folder": ".gradle",
          "reuploadOnChanges": true
        },
        "name": "gradle_local"
      },
      {
        "cacheInstruction": {
          "folder": "$HOME/.gradle",
          "reuploadOnChanges": true
        },
        "name": "gradle_global"
      },
      {
        "name": "Upload 'gradle_local' cache",
        "uploadCacheInstruction": {
          "cacheName": "gradle_local"
        }
      },
      {
        "name": "Upload 'gradle_global' cache",
        "uploadCacheInstruction": {
          "cacheName": "gradle_global"
        }
      }
    ],
    "environment": {
      "CIRRUS_OS": "linux"
    },
    "instance": {
      "@type": "type.googleapis.com/org.cirruslabs.ci.services.cirruscigrpc.ContainerInstance",
      "cpu": 2,
      "image": "debian:latest",
      "memory": 4096
    },
    "metadata": {
      "properties": {
        "allow_failures": "false",
        "auto_cancellation": "false",
        "experimental": "false",
        "indexWithinBuild": "0",
        "skip_notifications": "false",
        "timeout_in": "3600",
        "trigger_type": "AUTOMATIC"
      }
    },
    "name": "main"
  }
]
