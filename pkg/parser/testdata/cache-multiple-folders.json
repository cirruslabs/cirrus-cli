[
  {
    "commands": [
      {
        "cloneInstruction": {},
        "name": "clone"
      },
      {
        "cacheInstruction": {
          "folders": [
            "first/",
            "second/",
            "third/"
          ],
          "reuploadOnChanges": true
        },
        "name": "main"
      },
      {
        "name": "Upload 'main' cache",
        "uploadCacheInstruction": {
          "cacheName": "main"
        }
      }
    ],
    "environment": {
      "CIRRUS_OS": "linux"
    },
    "instance": {
      "@type": "type.googleapis.com/org.cirruslabs.ci.services.cirruscigrpc.ContainerInstance",
      "cpu": 2,
      "image": "debian:latest",
      "memory": 4096
    },
    "metadata": {
      "properties": {
        "allow_failures": "false",
        "experimental": "false",
        "indexWithinBuild": "0",
        "timeout_in": "3600",
        "trigger_type": "AUTOMATIC"
      }
    },
    "name": "main"
  }
]
